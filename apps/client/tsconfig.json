{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "module": "ESNext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src"]
}